{
  "name": "@progress/kendo-angular-examples-common",
  "description": "Kendo UI Angular component starter template",
  "version": "0.0.0-semantically-released",
  "author": "progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/npm/index.js",
  "esm5": "dist/es/index.js",
  "esm2015": "dist/es2015/index.js",
  "jsnext:main": "dist/es/index.js",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "typings": "dist/es2015/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build-cdn": "gulp build-cdn",
    "build-package": "gulp build-package",
    "test": "jest",
    "test:ci": "ci-angular-test",
    "test:watch": "jest --watch",
    "lint": "eslint . --quiet",
    "prepare": "npx ngcc"
  },
  "keywords": [
    "Kendo UI",
    "Angular"
  ],
  "peerDependencies": {
    "@angular/common": "12 - 14",
    "@angular/core": "12 - 14",
    "@angular/router": "12 - 14",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "^12.2.7",
    "@angular-devkit/schematics": "^12.2.7",
    "@angular/animations": "^12.2.7",
    "@angular/common": "^12.2.7",
    "@angular/compiler": "^12.2.7",
    "@angular/compiler-cli": "^12.2.7",
    "@angular/core": "^12.2.7",
    "@angular/forms": "^12.2.7",
    "@angular/localize": "^12.2.7",
    "@angular/platform-browser": "^12.2.7",
    "@angular/platform-browser-dynamic": "^12.2.7",
    "@angular/platform-server": "^12.2.7",
    "@angular/router": "^12.2.7",
    "@progress/kendo-angular-tasks": "^20.0.11",
    "@progress/semantic-prerelease": "^3.0.0",
    "@types/jest": "ts4.3",
    "@types/node": "ts4.3",
    "core-js": "^3.18.0",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "^4.0.0",
    "jest": "^27.2.2",
    "jest-junit": "^12.3.0",
    "rxjs": "^6.5.3",
    "semantic-release": "^6.3.6",
    "typescript": "~4.3.4",
    "validate-commit-msg": "^1.1.1",
    "zone.js": "~0.11.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-angular-examples-common.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular-examples-common"
  },
  "homepage": "https://www.telerik.com/kendo-angular-ui/components/",
  "release": {
    "debug": false,
    "branchTags": {},
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@progress/semantic-prerelease/analyzeCommits",
    "generateNotes": "@progress/semantic-prerelease/generateNotes",
    "getLastRelease": "@progress/semantic-prerelease/getLastRelease",
    "verifyConditions": "@progress/semantic-prerelease/verifyConditions",
    "verifyRelease": "@progress/semantic-prerelease/verifyRelease"
  }
}
